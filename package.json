{
  "name": "freelance_market",
  "private": true,
  "packageManager": "pnpm@9.15.2",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "dev:server": "concurrently \"pnpm -C packages/database db:generate\" \"pnpm -C packages/database dev\" \"pnpm -C packages/shared-types dev\" \"pnpm -C apps/server dev\"",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types"
  },
  "devDependencies": {
    "concurrently": "^9.1.2",
    "dotenv-cli": "^8.0.0",
    "ts-morph": "^25.0.1",
    "turbo": "^2.5.3"
  }
}
